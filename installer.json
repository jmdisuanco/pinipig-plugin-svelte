{
  "version": "0.1",
  "name": "pinipigSvelte",
  "pluginVersion": "0.0.1",
  "public": true,
  "folderName": "svelte",
  "workingDir": "svelte",
  "routeEject": "root",
  "devDependencies": [
    "@fullhuman/postcss-purgecss",
    "npm-run-all",
    "postcss-cli",
    "rollup",
    "rollup-plugin-commonjs",
    "rollup-plugin-livereload",
    "rollup-plugin-node-resolve",
    "rollup-plugin-svelte",
    "rollup-plugin-terser",
    "svelte",
    "es6-promise",
    "isomorphic-fetch",
    "tailwindcss"
  ],
  "scripts": {
    "svelte:autobuild": "rollup -c ./src/plugins/svelte/config/rollup.config.js -w",
    "svelte:autobuild:ssr": "rollup -c ./src/plugins/svelte/config/rollup.ssr.config.js -w",
    "svelte:dev": "run-p svelte:watch:routes svelte:autobuild svelte:watch:tailwind svelte:autobuild:ssr dev",
    "build": "run-p svelte:build:tailwind  svelte:build:csr",
    "svelte:build:csr": "rollup -c ./src/plugins/svelte/config/rollup.config.js",
    "svelte:build:ssr": "rollup -c ./src/plugins/svelte/config/rollup.ssr.config.js ",
    "svelte:watch:tailwind": "postcss --config ./src/plugins/svelte/config public/assets/tailwind.css -o public/assets/index.css -w",
    "svelte:build:tailwind": "NODE_ENV=production postcss --config ./src/plugins/svelte/config public/assets/tailwind.css -o public/assets/index.css",
    "svelte:build:routes": "node ./src/plugins/svelte/bin/genRoutes",
    "svelte:watch:routes": "node ./src/plugins/svelte/bin/genRoutes -w"
  },
  "dependencies": "",
  "repository": "https://github.com/jmdisuanco/pinipig-plugin-svelte",
  "required": "",
  "routes": {},
  "model": {}
}
